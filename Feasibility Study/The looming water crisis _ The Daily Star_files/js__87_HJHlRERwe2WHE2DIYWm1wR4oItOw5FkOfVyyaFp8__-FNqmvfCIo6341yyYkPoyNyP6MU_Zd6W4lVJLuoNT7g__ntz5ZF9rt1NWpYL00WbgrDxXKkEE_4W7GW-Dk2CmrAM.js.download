  $(function() {
    $(document).on('click', '.show-comments', function(e) {
      e.preventDefault();
      var data = $(this).data();
      var html = '<div class="fb-comments" data-href="' + data.url + '" data-numposts="5" data-width="100%"></div>';
      var $el = $(data.target);
      if($el.hasClass('hide')) {
        $el.removeClass('hide').append(html);
        FB.XFBML.parse($el[0]);
      }
    });
  });

})(jQuery);


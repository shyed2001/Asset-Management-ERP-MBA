(function($) {

  window.rsiScroller = {
    nid: 0,
    tid: 0,
    last: 0,
    titleSuffix: '',
    target: $('#news-scroller'),
    loading: false,
    gaTracked: [],

    init: function() {
      this.nid = this.target.data('nid');
      this.tid = this.target.data('tid');

      var parts = document.title.split(' | ');
      this.titleSuffix = ' | ' + parts[1];

      var data = {title: parts[0], url: document.location.href};
      $('#node-' + this.nid).parents('.row.clearfix').addClass('rsi-scroller-content').data(data);

      this.loadWaypoint();
    },

    loadWaypoint: function() {
      $('.main').waypoint({
        handler: function() {
          rsiScroller.loadContent();
        },
        offset: 'bottom-in-view'
      });

      $('.rsi-scroller-content').each(function() {
        var item = $(this);

        item.waypoint({
          handler: function(direction) {
            if(direction == 'up') {
              rsiScroller.setAttr($(this.element).data());
            }
          },
          offset: 'bottom-in-view'
        });

        item.waypoint({
          handler: function(direction) {
            if(direction == 'down') {
              rsiScroller.setAttr($(this.element).data());
            }
          },
          offset: '50%'
        });
      });
    },

    loadContent: function() {
      if(this.tid && !this.loading) {
        this.loading = true;
        var url = '/rsi-scroller/' + (this.last ? this.last : this.nid) + '/' + this.tid;
        $.getJSON(url, function(data) {
          for(var i in data) {
            rsiScroller.last = data[i].nid;
            var html = '<div class="rsi-scroller-content" data-nid="' + data[i].nid + '"  data-title="' + data[i].title + '"  data-url="' + data[i].url + '" data-tid="' + rsiScroller.tid + '" data-created="' + data[i].created + '" data-type="' + data[i].type + '">' + data[i].content + '</div>';
            rsiScroller.target.append(html);
          }

          setTimeout(function() {
            rsiScroller.loadWaypoint();
            rsiScroller.refreshAddthis();
            window.reInitLightGallery();
            window.rsiDfpLoader.init();

            if($(".stick-right-section").length){
              $(".stick-right-section").stick_in_parent({
                spacer: false,
                offset_top: $("#header-sticky").outerHeight() + 20,
                parent: ".stick-parents"
              });
            }

          }, 200);

          setTimeout(function() {
            rsiScroller.loading = false;
          }, 1000);
        });
      }
    },

    refreshAddthis: function() {
      if(typeof addthis !== 'undefined') {
        addthis.layers.refresh();
      }
    },

    setAttr: function(data) {
      window.history.replaceState(data, data.title, data.url);
      $(document).prop('title', data.title + this.titleSuffix);

      if(typeof data.nid !== 'undefined' && typeof ga !== 'undefined' && this.gaTracked.indexOf(data.nid) === -1) {
        ga('set', 'dimension1', 'News');
        ga('set', 'dimension2', data.nid);
        ga('set', 'dimension3', data.tid);
        ga('set', 'dimension4', 'english');
        ga('set', 'dimension5', data.created);
        ga('set', 'dimension6', data.type);
        ga('set', 'page', data.url);
        ga('send', 'pageview');
        ga('send', 'event', 'Scroll Pageview', data.url);
        this.gaTracked.push(data.nid);
      }
    }

  };

  $(function() {
    window.rsiScroller.init();
  });

})(jQuery);

